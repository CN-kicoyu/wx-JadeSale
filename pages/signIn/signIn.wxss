page {
  background: url('http://oo3thr1aa.bkt.clouddn.com/signIn.jpg') no-repeat 0 0;
  width: 100%;
  height: 100%;
  background-size: cover;
  overflow: hidden;
}

.signin-avator {
  width: 90rpx;
  height: 108rpx;
  margin: 126rpx auto 0;
  margin-bottom: 132rpx;
}

.signin-avator > image {
  width: 100%;
  height: 100%;
}

.signin-wrap {
  width: 558rpx;
  margin: 42rpx auto;
}

.signin-wrap >text {
  color: white;
  margin-bottom: 6rpx;
}

.user {
  position: relative;
}

.user >image {
  width: 26rpx;
  height: 26rpx;
  position: absolute;
  top: 18rpx;
  left: 0;
}

.sign-input {
  border-bottom: 1px solid white;
  padding: 10rpx 40rpx;
  font-size: 26rpx;
  margin: 10rpx 0;
  color: white;
}

.signin-password {
  color: white;
  width: 558rpx;
  margin: 0 auto;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.signin-password > label {
  color: white;
}

button[type="default"][plain].btn {
  width: 558rpx;
  margin-top: 75rpx;
  color: white;
  border: 1px solid white;
  font-size: 26rpx;
  padding: 20rpx 0;
}

button[type="default"][plain].btn.button-hover {
  background-color: white;
  color: white;
}
.other {
  margin-top:150rpx;
}
.infos {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  width:500rpx;
  margin:0 auto;
}
.hr {
  width: 100rpx;
  height:1px;
  background: white;
}
.text {
  margin:0 30rpx;
  text-align:center;
  flex:1;
  color: white;
}
.imgicons {
  display: flex;
  flex-direction: row;
  justify-content: center;
  margin-top:50rpx;
  align-items: center;
}
.weixin {
  width:50rpx;
  height:42rpx;
  margin-right:30rpx;
}
.qq {
  width:32rpx;
  height:36rpx;
}