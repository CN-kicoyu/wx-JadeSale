page {
  background: white;
}
.travel {
  width:100%;
  height:300rpx;
  position:relative;
}
.travel > image {
  width:100%;
  height:100%;
  position:absolute;
  top:0;
  left:0;
  z-index:-1;
}
.travel-place {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding-top:54rpx;
}
.place {
  width:144rpx;
  height:127rpx;
  position:relative;
  text-align:center;
  margin-bottom:21rpx;
}
.place > image {
  width:100%;
  height:100%;
}
.place > text {
  display: block;
  width:100%;
  color:white;
  font-size:20rpx;
  position:absolute;
  bottom:2rpx;
  left:0;
  text-align:center;
}
.travel-wrap {
  border-left:2rpx solid #f2f2f2;
  margin:0 30rpx;

}
.wrap {
  padding-top:30rpx;
}
.wrap-name {
  position:relative;
  left:-20rpx;
  background: white;
  margin-bottom:26rpx;
  color:#0c95ff;
}
.wrap-place {
  display: flex;
  flex-direction: row;
}
.icon {
  width:26rpx;
  height:26rpx;
  position: relative;
  left:-13rpx;
}
.text {
flex:1;
}
.text > image {
  width:100%;
  height:366rpx;
  margin-top:15rpx;
}
.imgs {
  display: flex;
  flex-direction: row;
  margin-top:15rpx;
}
.imgs >image {
  width:214rpx;
  height:144rpx;
  margin-right:10rpx;
}
.imgs >image:last-child {
  margin-right: 0;
}