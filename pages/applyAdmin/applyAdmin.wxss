.wrap {
  height:226rpx;
  border-bottom:1px solid #dedede;
  padding:45rpx 0;
  text-align:center;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.title {
  width: 240rpx;
  color:#00a0e9;
  font-size:30rpx;
  display: block;
  margin:0 auto;
}
.hr {
  width:100rpx;
  height: 4rpx;
  border-radius:100px;
  display: block;
  margin:30rpx auto 0;
  background:#ffcf55;
}
.wrap > image {
  width: 95rpx;
  height:90rpx;
  margin-bottom:20rpx;
}
.white {
  background: white;
}