.welfare-banner {
    width:100%;
    height:280rpx;
    font-size:0;
    margin:0;
    padding:0;
}


.welfare-activity {
    width:335rpx;
    box-shadow: 0 1px 3px rgba(150,150,150,0.4);
    background:#fff;
    text-align:center;
    border-radius:4rpx;
    overflow:hidden;
    margin:20rpx 0 10rpx 30rpx;
    float:left;
}

.welfare-activity > image {
    width:335rpx;
    height:266rpx;
}

.welfare-activity {
    color:#333333;
    font-size:26rpx;
}

.welfare-activity-btn {
    display:block;
    border-top:1px solid #dededeccc;
    color:#ffcf55;
    font-weight:bold;
    margin-top:30rpx;
    padding:26rpx 0;
}
.y-cl {
    height:20rpx;
}