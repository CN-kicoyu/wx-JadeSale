
.basket-none-wrap {
  display: flex;
  flex-direction: column;
  align-items: center;
  color: #ccc;
  padding-bottom:29%;
  background:#fff;
}

.basket-none {
  width: 200rpx;
  height: 200rpx;
  margin: 29% auto 30rpx;
}

.name {
  border-left: 4rpx solid #ffcf55;
  padding-left: 15rpx;
  display: block;
}
.fhead {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.edit {
  width: 22rpx;
  height:22rpx;
}
.wrap {
  background: white;
  border-radius: 4rpx;
  margin:20rpx 30rpx;
}
.title {
  padding:20rpx 30rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  border-bottom:1px solid #dedede;
}
.forms {
  margin:20rpx 30rpx 50rpx;
}
.fhead {
  display: flex;
  flex-direction: row;
  justify-content: center;
  padding:20rpx 10rpx;
  border-bottom:1px solid #dedede;
}
.fhead:last-child {
  border: 0;
}
.fhead > text, .fhead > view {
  flex:1;
  text-align: center;
}
.bg {
  background: #dedede;
}
.input {
  border:1px solid #dedede;
padding:5rpx 10rpx;
text-align: left;
display: block;
width:60%;
margin:0 auto;
}