.new-wrap {
    padding-bottom:23rpx;
    border-bottom:1px dashed #ccc;
    margin-bottom:30rpx;
}
.new-wrap:last-child {
    border:none;
}
.new-img {
    width:100%;
    height:260rpx;
    margin-bottom:20rpx;  
}
.new-detail {
    display:flex;
    flex-direction: row;
    justify-content:space-between;
    align-items:center;
    font-size:20rpx;
    color:#999;
    margin-top:10rpx;
}
.new-star {
    width:18rpx;
    height:18rpx;
    margin-right:6rpx;
}